{
  "options": {
    "package_manager_override": "pacman"
  },
  "desktops": [
    {
      "name": "GNOME (Wayland)",
      "command": "env GNOME_SESSION_DISABLE_USER_SYSTEMD=1 XDG_SESSION_TYPE=wayland dbus-run-session -- gnome-shell --wayland",
      "session_type": "wayland",
      "detect_commands": ["gnome-shell"],
      "install_command": "sudo pacman -S --needed gnome",
      "install_extras": [
        {
          "name": "GNOME Extra Apps",
          "command": "sudo pacman -S --needed gnome-extra",
          "description": "Extra GNOME applications"
        },
        {
          "name": "GNOME Tweaks",
          "command": "sudo pacman -S --needed gnome-tweaks"
        }
      ],
      "uninstall_command": "sudo pacman -Rns gnome",
      "uninstall_extras": [
        {
          "name": "Remove GNOME Extra Apps",
          "command": "sudo pacman -Rns gnome-extra"
        },
        {
          "name": "Remove GNOME Tweaks",
          "command": "sudo pacman -Rns gnome-tweaks"
        }
      ],
      "show_in_configured": false,
      "description": "Full GNOME Shell session on Wayland"
    },
    {
      "name": "GNOME (X11)",
      "command": "startx /usr/bin/gnome-session -- :1 vt$XDG_VTNR",
      "session_type": "x11",
      "detect_commands": ["gnome-session"],
      "install_command": "sudo pacman -S --needed gnome",
      "uninstall_command": "sudo pacman -Rns gnome",
      "show_in_configured": false,
      "description": "Run GNOME session under Xorg"
    },
